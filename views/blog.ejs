	<script>
	function myFunction() {


var input = $('#myInput').val();


if (input != '') {
	console.log(input);
	$.ajax({
		type: "post",
		url: "/searchProduct",
		data: { input: input },
		dataType: 'json',
		encode: true
	}).done(function (data) {
		console.log(data);
		$("#resultSearch").html('');
		$('#iconSearch').innerHTML = '<i class="fs-12 fa fa-circle-o-notch" aria-hidden="true"></i>';
		data.forEach(element => {
			var result = '<div class="divider">__</div>';
			var result2 = '<div class="search-product pos-relative bo4 of-hidden">' + '<img src="' + element.img[0] + '" width="30" height="40" style="background-position:  center center;margin: auto 5px; background-size: cover;"  >' + '<span>' + element.title + '</span>' + ' </div>';

			$("#resultSearch").append(result + result2);

		});




	})
}
else {
	$("#resultSearch").html('');
}
}
	</script>

