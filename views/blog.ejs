		<div class="container">
			<div class="row">
				<div class="col-md-8 col-lg-9 p-b-75">
					<div class="p-r-50 p-r-0-lg">
						<!-- item blog -->

						<!-- item blog -->


						<!-- item blog -->

						<!-- item blog -->

						<% articles.forEach(function(article){ %>

						<!-- item blog -->
						<div class="item-blog p-b-80">
							<a href="" class="item-blog-img pos-relative dis-block hov-img-zoom">
								<img src="<%= article.img %>" style="width:820px; height: 720px" height="" alt="IMG-BLOG">

								<span class="item-blog-date dis-block flex-c-m pos1 size17 bg4 s-text1">
									<%= article.date.getDate() %>/
									<%= article.date.getMonth() %>/
									<%= article.date.getFullYear() %>
								</span>
							</a>

							<div class="item-blog-txt p-t-33">
								<h4 class="p-b-11">
									<a href="blog-detail.html.<%= article._id %>" class="m-text24">
										<%= article.title %>
									</a>
								</h4>

								<div class="s-text8 flex-w flex-m p-b-21">
									<span>
										By Admin
										<span class="m-l-3 m-r-6">|</span>
									</span>

									<span>
										<%= article.tags %>
										<span class="m-l-3 m-r-6">|</span>
									</span>

									<span>
										<%= article.comment.length %>
									</span>
								</div>

								<p class="p-b-12">
									<%= article.des.toString().slice(0,150) %>
									<span>......</span>
								</p>

								<a href="blog-detail.html.<%= article._id %>" class="s-text20">
									Continue Reading
									<i class="fa fa-long-arrow-right m-l-8" aria-hidden="true"></i>
								</a>
							</div>
						</div>
						<%}) %>
					</div>

					<!-- Pagination -->
					<div class="pagination flex-m flex-w p-t-26">
							<% if (pages > 0) { %>
							<ul class="pagination text-center ">
								<% if (current == 1) { %>
								<li class="disabled item-pagination flex-c-m trans-0-4"><a>First</a></li>
								<% } else { %>
								<li class=" item-pagination flex-c-m trans-0-4 "><a href="/blog.html.page=1">First</a></li>
	
								<% } %>
								<% var i = (Number(current) > 5 ? Number(current) - 4 : 1) %>
								<% if (i !== 1) { %>
								<li class="disabled"><a>...</a></li>
								<% } %>
								<% for (; i <= (Number(current) + 4) && i <= pages; i++) { %>
								<% if (i == current) { %>
								<li class=" item-pagination flex-c-m trans-0-4 active-pagination"><a>
										<%= i %></a></li>
								<% } else { %>
								<li class=" item-pagination flex-c-m trans-0-4 "><a href="/blog.html.page=<%= i %>">
										<%= i %></a></li>
								<% } %>
	
								<% } %>
								<% if (current == pages) { %>
								<li class="disabled item-pagination flex-c-m trans-0-4"><a>Last</a></li>
								<% } else { %>
								<li class="item-pagination flex-c-m trans-0-4 "><a href="/blog.html.page=<%= pages %>">Last</a></li>
